<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="musicTypeFragment"
            type="com.example.thangpham.mvvcmusicapp.view.fragment.MusicTypeFragment"/>
    </data>
    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.fragment.MusicTypeFragment">

        <android.support.v7.widget.RecyclerView
            android:setAdapter="@{musicTypeFragment.musicTypeAdapterObservableField}"
            android:id="@+id/rl_listMusicType"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.RecyclerView>

    </FrameLayout>
</layout>